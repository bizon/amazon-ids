{
  "name": "@bizon/amazon-ids",
  "version": "5.0.1",
  "description": "Collection of Amazon identifiers",
  "repository": "https://github.com/bizon/amazon-ids",
  "author": "Bertrand Marron <bertrand.marron@gmail.com>",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "check:ts": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@bizon/semantic-release-config": "^3.1.0",
    "@jest/types": "^30.2.0",
    "@swc/core": "^1.15.10",
    "@swc/jest": "^0.2.39",
    "@tsconfig/node24": "^24.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.7",
    "eslint-config-xo-bizon": "^4.2.0",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "semantic-release": "^25.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
  "keywords": [
    "amazon",
    "sellercentral",
    "vendorcentral",
    "marketplaces"
  ],
  "release": {
    "extends": "@bizon/semantic-release-config"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "dts": true
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
